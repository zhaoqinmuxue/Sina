<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NineGridImageView">
        <attr format="dimension" name="singleImgSize"/>
        <attr format="dimension" name="imgGap"/>
        <attr format="integer" name="maxSize"/>
        <attr format="enum" name="showStyle">
            <enum name="grid" value="0"/>
            <enum name="fill" value="1"/>
        </attr>
    </declare-styleable>

    <attr name="statusBack" format="color"/>
    <attr name="statusFore" format="color"/>
    <attr name="mainBack" format="color"/>
    <attr name="secondaryBack" format="color"/>
    <attr name="mainFore" format="color"/>
    <attr name="lightMainFore" format="color"/>
    <attr name="secondaryFore" format="color"/>
    <attr name="accentFore" format="color"/>
    <attr name="accentBack" format="color"/>
</resources>